<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminBooking2">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".AdminBooking">

        <Button
            android:id="@+id/button20"
            android:layout_width="133dp"
            android:layout_height="51dp"
            android:layout_weight="1"
            android:backgroundTint="#47BD34"
            android:onClick="previousMonthAction"
            android:text="UMPSA"
            android:textColor="@color/white"
            android:textSize="20dp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="224dp"
            tools:layout_editor_absoluteY="220dp" />

        <LinearLayout
            android:layout_width="413dp"
            android:layout_height="81dp"
            android:background="#D9D9D9"
            android:backgroundTint="#D9D9D9"
            android:orientation="vertical"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="68dp"></LinearLayout>

        <LinearLayout
            android:layout_width="426dp"
            android:layout_height="71dp"
            android:background="#08466A"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="-5dp">

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="BOOKING"
                android:textColor="#FFFFFF"
                android:textSize="34sp"
                android:textStyle="bold" />


        </LinearLayout>

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="BOOKING LIST"
            android:textSize="20sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="129dp"
            tools:layout_editor_absoluteY="170dp" />

        <Button
            android:id="@+id/button17"
            android:layout_width="133dp"
            android:layout_height="51dp"
            android:layout_weight="1"
            android:backgroundTint="#08466A"
            android:onClick="previousMonthAction"
            android:text="DHUAM"
            android:textColor="@color/white"
            android:textSize="20dp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="220dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>